{% extends layouts.default %}

{% block page %}
	{% if flexible %}
		{% for item in flexible %}
			{% include paths.components ~ '/flexible/' ~ item.name ~ '/_' ~ item.name ~ '.twig' with {
				ce: item
			} %}
		{% endfor %}
	{% endif %}
{% endblock %}
