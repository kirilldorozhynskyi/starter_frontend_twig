{% extends '../layouts/main.twig' %}

{% block page %}
	{% if flexible %}
		{% for item in flexible %}
			{% include '../components/flexible/' ~ item.name ~ '/_' ~ item.name ~ '.twig' with {
				ce: item
			} %}
		{% endfor %}
	{% endif %}
{% endblock %}
